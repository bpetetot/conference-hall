{
  "name": "conference-hall",
  "version": "0.20.1",
  "private": true,
  "bugs": "https://github.com/bpetetot/conference-hall/issues",
  "repository": "github:bpetetot/conference-hall",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --maxWorkers=2",
    "test:debug": "react-scripts --inspect-brk test --runInBand --env=jsdom",
    "lint": "eslint --ext js,jsx src",
    "size": "bundlesize",
    "doc": "NODE_PATH=./src docz dev",
    "doc:build": "NODE_PATH=./src docz build",
    "changelog": "gitmoji-changelog"
  },
  "dependencies": {
    "@k-ramel/react": "^2.1.0",
    "@k-redux-router/core": "^1.0.0",
    "@k-redux-router/react-k-ramel": "^1.2.0",
    "classnames": "^2.2.6",
    "date-fns": "^1.30.1",
    "final-form": "^4.18.2",
    "final-form-arrays": "^3.0.1",
    "firebase": "^6.3.3",
    "font-awesome": "^4.7.0",
    "hoc-react-loader": "^6.3.0",
    "immutadot": "^1.0.0",
    "intl-messageformat": "^6.0.0",
    "k-ramel": "^2.1.0",
    "lodash": "^4.17.15",
    "luxon": "^1.17.2",
    "marked": "^0.7.0",
    "normalize.css": "^8.0.1",
    "path-to-regexp": "^3.0.0",
    "popper.js": "^1.15.0",
    "prop-types": "^15.7.2",
    "randomcolor": "^0.5.4",
    "react": "^16.8.6",
    "react-datepicker": "^2.8.0",
    "react-dom": "^16.8.6",
    "react-final-form": "^6.3.0",
    "react-final-form-arrays": "^3.1.0",
    "react-places-autocomplete": "^7.2.1",
    "react-sizes": "^2.0.0",
    "redux": "^4.0.4",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "bundlesize": "^0.18.0",
    "docz": "^1.2.0",
    "docz-theme-default": "^1.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "enzyme-to-json": "^3.3.5",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "firebase-tools": "^7.2.2",
    "prettier": "^1.18.2",
    "react-scripts": "^3.0.1",
    "react-test-renderer": "^16.8.6",
    "redux-devtools-extension": "^2.13.8"
  },
  "resolutions": {
    "docz/**/webpack": "4.28.4"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb"
    ],
    "env": {
      "browser": true
    },
    "rules": {
      "semi": [
        2,
        "never"
      ],
      "import/no-extraneous-dependencies": 0,
      "import/no-unresolved": 0,
      "import/extensions": 0,
      "react/forbid-prop-types": 0,
      "react/jsx-one-expression-per-line": 0,
      "react/destructuring-assignment": 0,
      "jsx-a11y/label-has-for": 0,
      "jsx-a11y/click-events-have-key-events": 0,
      "jsx-a11y/interactive-supports-focus": 0,
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "aspects": [
            "noHref",
            "invalidHref",
            "preferButton"
          ]
        }
      ]
    }
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "bundlesize": [
    {
      "path": "./build/static/js/*.js",
      "maxSize": "350 kB"
    },
    {
      "path": "./build/static/css/*.css",
      "maxSize": "20 kB"
    }
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  }
}
