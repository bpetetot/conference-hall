{
  "name": "conference-hall",
  "version": "0.35.2",
  "private": true,
  "bugs": "https://github.com/bpetetot/conference-hall/issues",
  "repository": "github:bpetetot/conference-hall",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:emulator": "firebase emulators:exec --only firestore 'jest --roots config'",
    "lint": "eslint --ext js,jsx src",
    "size": "bundlesize",
    "doc": "start-storybook -p 6006 --docs",
    "doc:build": "build-storybook --docs",
    "changelog": "npx gitmoji-changelog@latest"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  },
  "dependencies": {
    "@k-ramel/react": "3.0.1",
    "classnames": "2.2.6",
    "date-fns": "2.16.1",
    "dompurify": "2.2.0",
    "final-form": "4.20.1",
    "final-form-arrays": "3.0.2",
    "firebase": "8.0.0",
    "font-awesome": "4.7.0",
    "history": "^5.0.0",
    "hoc-react-loader": "6.3.0",
    "immutadot": "1.0.0",
    "intl-messageformat": "9.3.12",
    "k-ramel": "3.0.1",
    "lodash": "4.17.20",
    "luxon": "1.25.0",
    "marked": "1.2.2",
    "normalize.css": "8.0.1",
    "path-to-regexp": "3.1.0",
    "popper.js": "1.16.1",
    "prop-types": "15.7.2",
    "randomcolor": "0.6.2",
    "react": "16.13.1",
    "react-datepicker": "3.3.0",
    "react-dom": "16.13.1",
    "react-final-form": "6.5.2",
    "react-final-form-arrays": "3.1.3",
    "react-places-autocomplete": "7.3.0",
    "react-router-dom": "^6.0.0-beta.0",
    "react-sizes": "2.0.0",
    "redux": "4.0.5",
    "uuid": "8.3.1",
    "validator": "13.1.17"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@firebase/testing": "0.20.11",
    "@storybook/addon-docs": "5.3.19",
    "@storybook/addons": "5.3.19",
    "@storybook/preset-create-react-app": "3.1.4",
    "@storybook/react": "5.3.19",
    "@testing-library/react-hooks": "3.4.2",
    "babel-loader": "8.1.0",
    "bundlesize": "0.18.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "enzyme-to-json": "3.6.1",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "firebase-tools": "8.14.1",
    "husky": "4.3.0",
    "prettier": "2.1.2",
    "react-is": "16.13.1",
    "react-scripts": "3.4.1",
    "react-test-renderer": "16.13.1",
    "redux-devtools-extension": "2.13.8"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "react-hooks"
    ],
    "extends": [
      "airbnb",
      "plugin:prettier/recommended"
    ],
    "env": {
      "browser": true
    },
    "rules": {
      "semi": [
        2,
        "never"
      ],
      "import/no-extraneous-dependencies": 0,
      "import/no-unresolved": 0,
      "import/extensions": 0,
      "react/forbid-prop-types": 0,
      "react/jsx-one-expression-per-line": 0,
      "react/destructuring-assignment": 0,
      "react/state-in-constructor": 0,
      "react/jsx-props-no-spreading": 0,
      "react/jsx-wrap-multilines": 0,
      "jsx-a11y/label-has-for": 0,
      "jsx-a11y/click-events-have-key-events": 0,
      "jsx-a11y/interactive-supports-focus": 0,
      "jsx-a11y/label-has-associated-control": 0,
      "jsx-a11y/control-has-associated-label": 0,
      "react-hooks/rules-of-hooks": 0,
      "react-hooks/exhaustive-deps": 1,
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "aspects": [
            "noHref",
            "invalidHref",
            "preferButton"
          ]
        }
      ]
    }
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "bundlesize": [
    {
      "path": "./build/static/js/*.js",
      "maxSize": "365 kB"
    },
    {
      "path": "./build/static/css/*.css",
      "maxSize": "20 kB"
    }
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  }
}
